{
  "name": "spark-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "depgraph": "npx depcruise src --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph.svg"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^11.0.3",
    "axios": "^1.7.7",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.4.4",
    "gsap": "^3.12.5",
    "normalize.css": "^8.0.1",
    "pinia": "^2.2.2",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.3",
    "vue-chartjs": "^5.3.1",
    "vue-debounce": "^5.0.0",
    "vue-router": "^4.4.3",
    "vue-select": "^4.0.0-beta.6",
    "vue-tel-input": "^9.1.4",
    "vue-toastification": "^2.0.0-rc.5",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.5.4",
    "@types/vue-select": "^3.16.8",
    "@types/vue-tel-input": "^2.1.7",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/tsconfig": "^0.5.1",
    "dependency-cruiser": "^16.4.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^3.4.0",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.6",
    "vue-tsc": "^2.1.6"
  }
}
